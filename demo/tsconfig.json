{
  "compilerOptions": {
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "sourceMap": true,
    "target": "es5",
    "noImplicitAny": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "dist",
    "moduleResolution": "node",
    "lib": ["es2017", "dom"],
    "jsx": "react",
    "esModuleInterop": true
  },
  "include": ["./custom.d.ts", "**/*.tsx", "**/*.ts"]
}
